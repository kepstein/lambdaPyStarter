{
  "name": "lambdaPyStarter",
  "description": "A lightweight starter kit for Python based Lambda functions",
  "region": "us-east-1",
  "handler": "lambda_function.lambda_handler",
  "role": "arn:aws:iam::00000000000:role/lambda_basic_execution",
  "requirements": [ ],
  "ignore": [
    ".env",
    ".git",
    "/*.pyc",
    ".gitignore",
    "/*.zip",
    "/*.pyc",
    "events/*",
    "test/*",
    "lambda.json",
    "lambda.vpc.json",
    "/*.md",
    "requirements.txt"
  ],
  "timeout": 30,
  "memory": 128
}
